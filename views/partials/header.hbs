<!-- Header -->
  <div id="skiplink-container">
    <div>
      <a href="#content" class="skiplink">{{ skipLinkMessage }}</a>
    </div>
  </div>

  <div id="global-cookie-message">

    {{>cookieMessage }}

  </div>


  <header role="banner" id="global-header" class="{{{ headerClass }}}">
    <div class="header-wrapper">
      <div class="header-global">
        <div class="header-logo">
          <a href="{{{ homepageUrl }}}" title="{{ logoLinkTitle }}" id="logo" class="content">
            <img src="{{{ assetPath }}}images/gov.uk_logotype_crown_invert_trans.png?0.18.0" width="35" height="31" alt=""> {{{ globalHeaderText }}}
          </a>
        </div>
        {{{ insideHeader }}}
      </div>
      {{>propositionHeader}}
    </div>
  </header>
  <div id="global-header-bar">
  <div class="inner-block">
    <div class="header-bar"></div>
  </div>
</div>
<!-- End -->
